    FontColor #000000
    FontSize 10
}
skinparam rectangle {
    BackgroundColor #e2e5e7ff
    BorderColor #000000
    FontColor #000000
    FontSize 11
}
skinparam linetype ortho

allowmixing
left to right direction

rectangle "Generare întrebări" as genQ{
    component "DatasetGenerator" as Generator
    component "Prompts" as Prompts
    component "Validator" as Validator
}

rectangle "LLM" {
}

rectangle "TOON Corpus" {
}

Prompts --> Generator
LLM <-> Generator 
Generator --> Validator
Validator --> "TOON Corpus"

@enduml

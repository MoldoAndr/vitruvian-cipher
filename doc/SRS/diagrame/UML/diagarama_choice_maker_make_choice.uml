@startuml
skinparam componentStyle rectangle
skinparam shadowing false
skinparam defaultFontName Arial
skinparam defaultFontSize 10
skinparam backgroundColor #FFFFFF
skinparam nodesep 40
skinparam ranksep 30
skinparam component {
    BackgroundColor #e5eaeeff
    BorderColor #000000
    BorderThickness 1
    FontColor #000000
    FontSize 10
}
skinparam database {
    BackgroundColor #e0e7ebff
    BorderColor #000000
    FontColor #000000
    FontSize 10
}
skinparam rectangle {
    BackgroundColor #e2e5e7ff
    BorderColor #000000
    FontColor #000000
    FontSize 10
}
skinparam arrow {
    Color #000000
}
skinparam linetype ortho

allowmixing

rectangle "Inferență" as inf {
    component "API" as API
    component "Pipeline" as Pipeline
}

rectangle "Modele SecureBERT" as Models {
}

rectangle "Antrenare" {
    component "Train scripts" as Trainers
    component "Corpus" as Corpus
}

API --> Pipeline

Pipeline --> Models
Corpus --> Trainers
Trainers --> Models

@enduml
